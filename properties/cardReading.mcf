nu X(ownerNumber:Nat=0,isLockerClosed:Bool=false).(
[whereIsLockArm(horizontal)]X(ownerNumber,true) &&
[unlockArm]X(0,false) &&
forall n:Nat.val(n<maxUser)=>[cardRead(n)]X(if(isLockerClosed,ownerNumber,n),isLockerClosed) &&
forall n:Nat.val(n<maxUser)=>([cardRead(n)](val(n==ownerNumber))=>(mu Y.([!unlockArm]true && <true>true))) &&
[!(whereIsLockArm(horizontal) || exists n:Nat.cardRead(n) || unlockArm)]X(ownerNumber, isLockerClosed)
)