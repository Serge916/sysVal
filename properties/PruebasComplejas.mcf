nu X(pos:lockArmPosition=vertical,state:lockArmStatus=fixed).(
[unlockArm]X(pos,loose) &&
(val(state==loose)=>forall p:lockArmPosition.[whereIsLockArm(p).!lockArm*]<true*.lockArm>X(p,state)) &&
(val(state==fixed)=>forall p:lockArmPosition.[whereIsLockArm(p)][true*.lockArm]false) &&
[!unlockArm && !exists p:lockArmPosition.whereIsLockArm(p)]X(pos,state))